{
  "name": "script-player",
  "version": "0.1.8",
  "description": "",
  "main": "main/main.js",
  "scripts": {
    "watch": "webpack --watch",
    "build": "webpack --mode=production --node-env=production",
    "start": "electron .",
    "start:forge": "electron-forge start",
    "rebuild:arm64": "electron-rebuild --arch arm64",
    "package": "cross-env NODE_ENV=production electron-forge package",
    "make": "cross-env NODE_ENV=production electron-forge make --targets @electron-forge/maker-zip",
    "format": "prettier --write '**/*.{ts,tsx,js,json}'",
    "clean": "rm -rf dist",
    "lint": "eslint \"renderer/**/*.{ts,tsx}\"",
    "lint:fix": "npm run format && eslint --fix \"renderer/**/*.{ts,tsx}\" \"tests/**/*.{js,ts}\"",
    "test": "playwright test",
    "check": "tsc --noEmit",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "serve": "webpack serve"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@electron-forge/cli": "^6.1.0",
    "@electron-forge/maker-zip": "^6.1.1",
    "@playwright/test": "^1.32.2",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@webpack-cli/generators": "^3.0.1",
    "cross-env": "^7.0.3",
    "electron": "^24.0.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "playwright": "^1.32.2",
    "prettier": "2.8.7",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.3",
    "webpack": "^5.77.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.11.16",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "date-fns": "^2.29.3",
    "electron-squirrel-startup": "^1.0.0",
    "file-type": "^18.2.1",
    "immutability-helper": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0",
    "recoil": "^0.7.7"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "script_player"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux",
            "win32"
          ]
        }
      ]
    }
  },
  "alias": {
    "react/jsx-runtime.js": "react/jsx-runtime",
    "react/jsx-dev-runtime.js": "react/jsx-dev-runtime"
  }
}
